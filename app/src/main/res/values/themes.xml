<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.GyannProject" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="android:statusBarColor">@color/white</item>
        <!-- Set the status bar items color to black -->
        <item name="android:windowLightStatusBar">true</item>
    </style>


    <style name="Theme.GyannProject" parent="Base.Theme.GyannProject" />

    <style name="Theme.GyannProject.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.GyannProject.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.GyannProject.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>